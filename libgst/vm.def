operation SEND_IMMEDIATE n ( -- ) {
  const struct builtin_selector *bs = _gst_builtin_selectors[n];
  PREPARE_STACK ();
  EXPORT_REGS ();
  SEND_MESSAGE (bs->symbol, bs->numArgs);
  IMPORT_REGS ();
  FETCH (dispatch_vec);
}

operation SEND_SUPER_IMMEDIATE n ( -- ) {
  const struct builtin_selector *bs = _gst_builtin_selectors[n];
  PREPARE_STACK ();
  EXPORT_REGS ();
